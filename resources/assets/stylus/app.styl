//Axis Resets
border-box-html();
global-reset();
normalize-css();
print-styles();
//Axis Settings
$default-color = black;
$font-stack = 'Open Sans Condensed', sans-serif;
$font-color = black;
//Lost Settings
@lost flexbox flex;
@lost gutter 15px;
//Rupture Settings
rupture.mobile-cutoff = 450px;
//Custom Colors
$bluetrans = alpha($blue, 0.9);
$whitetrans = alpha($white,.9);
$blacktrans = alpha($black,.9);
$lightbrown = alpha(#BCAAA4,1);
//Page CSS
@import 'component/body';
@import 'component/navbar';
@import 'component/footer';
@import 'component/home';
@import 'component/blog';
@import 'component/sidebar';
@import 'component/cookies';
@import 'component/aboutme';
@import 'component/portfolio';
//Misc CSS
@import 'component/notifications';
@import 'component/fontawesome';
