//Axis Resets
border-box-html();
global-reset();
normalize-css();
print-styles();
//Axis Settings
$default-color = $blue;
$font-stack = 'Roboto Mono', monospace;
$font-color = $black;
//Lost Settings
@lost flexbox flex;
@lost gutter 15px;
//Rupture Settings
rupture.mobile-cutoff = 450px;
//Custom Colors
$bluetrans = alpha($blue, 0.9);
$whitetrans = alpha($white,.9);
$blacktrans = alpha($black,.9);
$darkgray = alpha(#727272,1);
//Page CSS
@import 'component/body';
@import 'component/navbar';
@import 'component/footer';
@import 'component/home';
@import 'component/blog';
@import 'component/sidebar';
@import 'component/cookies';
//Misc CSS
@import 'component/notifications';
@import 'component/fontawesome';
// Fonts etc. 
@import url(https://fonts.googleapis.com/css?family=Roboto+Mono);